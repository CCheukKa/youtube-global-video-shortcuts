{
    "manifest_version": 3,
    "name": "YouTube Global Video Shortcuts",
    "version": "1.0.0",
    "description": "Fixes YouTube keyboard shortcuts not being global",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}